#include <stdio.h>
#include <math.h>
int perfect(int num)
{
    int squareroot=pow(num,0.5);
}
int main() {
    int num;
    printf("Enter the number: ");
    scanf("%d",&num);
     if((perfect(num)*perfect(num))==num)
     {
         printf("Perfect square number");
     }
     else
     {
         printf("Not a perfect square number");
     }
    return 0;
}
