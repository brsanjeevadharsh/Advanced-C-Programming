#include <stdio.h>
int main()
{
    int size;
    printf("Enter the size of array: ");
    scanf("%d",&size);
    int array[size];
    printf("Enter the elements of an array: \n");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&array[i]);
    }
    for(int i=0;i<size;i++)
    {
        if(i%2!=0)
        {
            printf("%d ",array[i]);
        }
    }
}
