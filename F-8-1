#include <stdio.h>
int main()
{
    int num;
    int s=0;
    printf("Enter the number");
    scanf("%d",&num);
    while(num!=0)
    {
        int num1=num%10;
        int fact=1;
        for(int i=1;i<=num1;i++)
        {
            fact=fact*i;
        }
        num=num/10;
        s=s+fact;
    }
    printf("%d",s);
}
