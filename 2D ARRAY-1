#include <stdio.h>
int main()
{
    int row,col;
    // int sum=0;
    printf("Enter the row and column size:");
    scanf("%d %d",&row,&col);
    int arr[row][col];
    printf("Enter the elements");
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
     int sum=0;
    for(int i=0;i<row;i++)
    {
        for(int j=0;j<col;j++)
        {
            sum=sum+arr[i][j];
        }
        
    }
    printf("%d",sum);
}
