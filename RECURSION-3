#include <stdio.h>
#include <string.h>
int main()
{
    char ch[100];
    int count;
    scanf("%s", ch);
    int n=strlen(ch);
    for(int i=0;i<n;i++)
    {
        if(ch[i]!=ch[n-i-1]) {
            count++;
            break;
        }
    }
if(count==0)
{
    printf("Palindrome");
}
else
{
    printf("Not palindrome");
}
}
